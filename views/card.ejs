<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
  </head>
  <body>
    <h1>Hello, world!</h1>
    <%   const marks=(a,b,c)=>{
	return a*2+b*3+c*5;

    }  %>
   
    
    <div class="card" style="width: 18rem;">
	<!-- <img class="card-img-top" src="..." alt="Card image cap"> -->
	<div class="card-body">
	  <h5 class="card-title"><%= profileData.username %></h5>
	</div>
	<ul class="list-group list-group-flush">
	  <li class="list-group-item">Easy problem= <%= profileData.submitStats.acSubmissionNum[1].count%></li>
	  <li class="list-group-item">Medium problem= <%= profileData.submitStats.acSubmissionNum[2].count%></li>
	  <li class="list-group-item">Hard  problem= <%= profileData.submitStats.acSubmissionNum[3].count%></li>
	</ul>
	<div class="card-body">
	  <a  class="list-group-item">Marks= <%= marks(profileData.submitStats.acSubmissionNum[1].count,profileData.submitStats.acSubmissionNum[2].count,profileData.submitStats.acSubmissionNum[3].count)  %></a>
	</div>
      </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
  </body>
</html>